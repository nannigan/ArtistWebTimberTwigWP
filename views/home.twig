{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">

				<div class="article-body">
                  <h4> This is home.twig</h4>

                   <pre>{{dump(post.home_photo)}}</pre>
                   {% set home_img = TimberImage(post.home_photo) %}
                    <img src="{{home_img.src}}" alt="{{home_img.title}}">



				</div>
			</section>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}